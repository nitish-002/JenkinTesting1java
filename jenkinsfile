pipeline{
    agent any
    stages{
        stage('checkout'){
            steps{
                git url: 'https://github.com/nitish-002/JenkinTesting1java.git', branch: 'main'
                echo 'checkout completed'
                
            }
        }
        stage('build'){
            steps{
                echo 'build completed'
            }
        }
         stage('test'){
            steps{
                echo 'test completed'
            }
        }
         stage('deploy'){
            steps{
                echo 'deploy completed'
            }
        }
    }
}